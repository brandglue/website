{
  "name": "brandglue-website",
  "private": true,
  "license": "UNLICENSED",
  "repository": "https://github.com/brandglue/website",
  "scripts": {
    "analyze": "react-static build --analyze",
    "build": "react-static build",
    "check-format": "yarn run prettier --list-different",
    "check-types": "tsc",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "format": "yarn run prettier --write",
    "lint": "eslint --ignore-path .gitignore '*/**/*.{js,ts,tsx}' --fix && stylelint '*/**/*.tsx'",
    "pretest:e2e:ci": "npm run build",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "stage": "react-static build --staging",
    "start": "react-static start",
    "serve": "serve dist -p 3000",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node-modules/jest/bin/jest.js --runInBand --watch",
    "test:e2e": "is-ci \"test:e2e:ci\" \"test:e2e:dev\"",
    "test:e2e:ci": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:3000 cy:open",
    "validate": "yarn run check-types && yarn run check-format && yarn run lint && yarn run build && yarn run test:coverage && yarn run test:e2e:ci"
  },
  "dependencies": {
    "@reach/router": "1.2.1",
    "classnames": "2.2.6",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-hot-loader": "4.12.18",
    "react-is": "16.12.0",
    "react-lazy-load-image-component": "1.4.0",
    "react-static": "7.2.3",
    "react-static-plugin-reach-router": "7.2.3",
    "react-static-plugin-sitemap": "7.2.3",
    "react-static-plugin-styled-components": "7.2.2",
    "styled-components": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@cypress/webpack-preprocessor": "4.1.1",
    "@testing-library/cypress": "5.0.2",
    "@types/classnames": "2.2.9",
    "@types/node": "13.1.7",
    "@types/reach__router": "1.2.6",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-hot-loader": "4.1.1",
    "@types/react-lazy-load-image-component": "1.3.0",
    "@types/styled-components": "4.4.2",
    "@types/testing-library__cypress": "5.0.1",
    "@types/webpack-env": "1.15.0",
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "babel-plugin-styled-components": "1.10.6",
    "cypress": "3.8.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-cypress": "2.8.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "file-loader": "5.0.2",
    "husky": "3.1.0",
    "image-webpack-loader": "6.0.0",
    "is-ci": "2.0.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "lqip-loader": "2.2.0",
    "prettier": "1.19.1",
    "react-static-plugin-typescript": "7.2.3",
    "responsive-loader": "1.2.0",
    "serve": "11.3.0",
    "sharp": "0.23.4",
    "start-server-and-test": "1.10.6",
    "styled-system": "5.1.4",
    "stylelint": "12.0.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.9.0",
    "test-data-bot": "0.8.0",
    "ts-loader": "6.2.1",
