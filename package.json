{
  "name": "brandglue-website",
  "private": true,
  "license": "UNLICENSED",
  "repository": "https://github.com/brandglue/website",
  "scripts": {
    "analyze": "react-static build --analyze",
    "build": "react-static build",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --fix && stylelint '*/**/*.tsx'",
    "stage": "react-static build --staging",
    "start": "react-static start",
    "serve": "serve dist -p 3000"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.tsx": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@reach/router": "1.2.1",
    "classnames": "2.2.6",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.8.4",
    "react-ideal-image": "0.0.5",
    "react-static": "7.0.10",
    "react-static-plugin-reach-router": "7.0.10",
    "react-static-plugin-sitemap": "7.0.10",
    "react-static-plugin-styled-components": "7.0.10",
    "react-waypoint": "8.0.3",
    "styled-components": "4.2.0",
    "webpack-dev-server": "3.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@types/classnames": "2.2.7",
    "@types/node": "11.13.8",
    "@types/reach__router": "1.2.4",
    "@types/react": "16.8.15",
    "@types/react-dom": "16.8.4",
    "@types/react-hot-loader": "4.1.0",
    "@types/styled-components": "4.1.14",
    "@types/webpack-env": "1.13.9",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "babel-plugin-styled-components": "1.10.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.6.0",
    "husky": "2.2.0",
    "image-webpack-loader": "4.6.0",
    "lint-staged": "8.1.5",
    "lqip-loader": "2.2.0",
    "prettier": "1.17.0",
    "react-static-plugin-typescript": "7.0.10",
    "responsive-loader": "1.2.0",
    "serve": "11.0.0",
    "sharp": "0.22.1",
    "stylelint": "10.0.1",
    "stylelint-config-prettier": "5.1.0",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.6.0",
    "typescript": "3.5.0-dev.20190501",
    "webpack": "4.30.0"
  },
  "resolutions": {
    "react-static/webpack-dev-server": "~3.1.10"
  }
}
