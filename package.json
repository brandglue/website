{
  "name": "brandglue-website",
  "private": true,
  "license": "UNLICENSED",
  "repository": "https://github.com/brandglue/website",
  "scripts": {
    "build": "gatsby build",
    "check-format": "npm run prettier --list-different",
    "check-types": "tsc",
    "clean": "gatsby clean",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "develop": "gatsby develop",
    "format": "npm run prettier --write",
    "lint": "eslint --ignore-path .gitignore '*/**/*.{js,ts,tsx}' --fix && stylelint '*/**/*.tsx'",
    "pretest:e2e:ci": "npm run build",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx|yaml)\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node-modules/jest/bin/jest.js --runInBand --watch",
    "test:e2e": "is-ci \"test:e2e:ci\" \"test:e2e:dev\"",
    "test:e2e:ci": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:3000 cy:open",
    "validate": "npm run check-types && npm run check-format && npm run lint && npm run build && npm run test:coverage && npm run test:e2e:ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-types && lint-staged && npm run build"
    }
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "eslint"
    ],
    "*.tsx": [
      "stylelint"
    ],
    "**/*.+(js|json|ts|tsx|yaml)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "1.6.5",
    "@mdx-js/react": "1.6.5",
    "classnames": "2.2.6",
    "compass-vertical-rhythm": "1.4.5",
    "flexsearch": "0.6.32",
    "fluid-system": "1.0.9",
    "gatsby": "2.23.1",
    "gatsby-image": "2.4.6",
    "gatsby-plugin-breadcrumb": "10.0.0",
    "gatsby-plugin-flexsearch": "1.0.3",
    "gatsby-plugin-graphql-codegen": "2.7.1",
    "gatsby-plugin-mdx": "1.2.15",
    "gatsby-plugin-meta-redirect": "1.1.1",
    "gatsby-plugin-netlify": "2.3.13",
    "gatsby-plugin-netlify-cms": "4.3.5",
    "gatsby-plugin-react-helmet": "3.3.4",
    "gatsby-plugin-root-import": "2.0.5",
    "gatsby-plugin-sharp": "2.6.11",
    "gatsby-plugin-styled-components": "3.3.4",
    "gatsby-plugin-typegen": "2.2.0",
    "gatsby-plugin-typescript": "2.4.6",
    "gatsby-plugin-typography": "2.5.6",
    "gatsby-redirect-from": "0.2.4",
    "gatsby-remark-embed-video": "3.0.10",
    "gatsby-remark-images": "3.3.11",
    "gatsby-remark-instagram-embed": "1.0.0",
    "gatsby-remark-responsive-iframe": "2.4.6",
    "gatsby-source-filesystem": "2.3.11",
    "gatsby-transformer-sharp": "2.5.5",
    "gray-percentage": "2.0.0",
    "lodash": "4.17.15",
    "lodash-es": "4.17.15",
    "netlify-cms-app": "2.12.14",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-typography": "0.16.19",
    "styled-components": "5.1.1",
    "styled-icons": "10.11.0",
    "styled-system": "5.1.5",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@cypress/webpack-preprocessor": "5.4.1",
    "@testing-library/cypress": "6.0.0",
    "@types/classnames": "2.2.10",
    "@types/lodash-es": "4.17.3",
    "@types/mdx-js__react": "1.5.2",
    "@types/node": "14.0.11",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.0.0",
    "@types/styled-components": "5.1.0",
    "@types/styled-system": "5.1.9",
    "@types/styled-system__css": "5.0.11",
    "@types/testing-library__cypress": "5.0.5",
    "@types/typography": "0.16.3",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "babel-preset-gatsby": "0.4.8",
    "cypress": "4.7.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "husky": "4.2.5",
    "is-ci": "2.0.0",
    "jest": "26.0.1",
    "lint-staged": "10.2.9",
    "prettier": "2.0.5",
    "remark": "12.0.0",
    "remark-extract-frontmatter": "2.0.2",
    "remark-frontmatter": "2.0.0",
    "remark-parse": "8.0.2",
    "remark-stringify": "8.1.0",
    "start-server-and-test": "1.11.0",
    "stylelint": "13.6.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "test-data-bot": "0.8.0",
    "to-vfile": "6.1.0",
    "ts-loader": "7.0.5",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "yaml": "1.10.0"
  }
}
