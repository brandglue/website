{
  "name": "brandglue-website",
  "private": true,
  "license": "UNLICENSED",
  "repository": "https://github.com/brandglue/website",
  "scripts": {
    "build": "gatsby build",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc",
    "clean": "gatsby clean",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "develop": "gatsby develop",
    "format": "npm run prettier -- --write",
    "lint": "eslint --ignore-path .gitignore '*/**/*.{js,ts,tsx}' --fix && stylelint '*/**/*.tsx'",
    "local-cms": "npx netlify-cms-proxy-server",
    "pretest:e2e:ci": "npm run build",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx|yaml)\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node-modules/jest/bin/jest.js --runInBand --watch",
    "test:e2e": "is-ci \"test:e2e:ci\" \"test:e2e:dev\"",
    "test:e2e:ci": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:3000 cy:open",
    "validate": "npm run check-types && npm run check-format && npm run lint && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "(git-branch-is master && npm run validate) || true",
      "pre-merge-commit": "(git-branch-is master && npm run validate) || true"
    }
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "eslint"
    ],
    "*.tsx": [
      "stylelint"
    ],
    "**/*.+(js|json|ts|tsx|yaml)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "1.6.22",
    "@mdx-js/react": "1.6.22",
    "classnames": "2.3.1",
    "compass-vertical-rhythm": "1.4.5",
    "flexsearch": "0.7.21",
    "fluid-system": "1.1.3",
    "gatsby": "4.4.0",
    "gatsby-cli": "4.4.0",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-breadcrumb": "12.3.0",
    "gatsby-plugin-flexsearch": "1.0.3",
    "gatsby-plugin-force-trailing-slashes": "1.0.5",
    "gatsby-plugin-google-analytics": "4.4.0",
    "gatsby-plugin-graphql-codegen": "3.1.0",
    "gatsby-plugin-mdx": "3.4.0",
    "gatsby-plugin-mdx-frontmatter": "0.0.4",
    "gatsby-plugin-meta-redirect": "1.1.1",
    "gatsby-plugin-netlify": "4.0.0-next.0",
    "gatsby-plugin-netlify-cms": "6.4.0",
    "gatsby-plugin-react-helmet": "5.4.0",
    "gatsby-plugin-robots-txt": "1.6.14",
    "gatsby-plugin-root-import": "2.0.8",
    "gatsby-plugin-sharp": "4.4.0",
    "gatsby-plugin-sitemap": "5.4.0",
    "gatsby-plugin-styled-components": "5.4.0",
    "gatsby-plugin-typegen": "2.2.4",
    "gatsby-plugin-typescript": "4.4.0",
    "gatsby-plugin-typography": "4.4.0",
    "gatsby-redirect-from": "0.4.3",
    "gatsby-remark-embed-video": "3.1.1",
    "gatsby-remark-images": "6.4.0",
    "gatsby-remark-instagram-embed": "1.0.0",
    "gatsby-remark-responsive-iframe": "g5.4.0",
    "gatsby-source-filesystem": "4.4.0",
    "gatsby-transformer-sharp": "4.4.0",
    "gray-percentage": "2.0.0",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "netlify-cms-app": "2.15.63",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-typography": "0.16.20",
    "styled-components": "5.3.3",
    "styled-icons": "10.45.0",
    "styled-system": "5.1.5",
    "typography": "0.16.21"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
